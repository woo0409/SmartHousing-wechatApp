input{
  border: black solid 1px;
  width: 50%;
  height: 80rpx;
  font-size: 30rpx;
  float: left;
}
.buttons{
  width: 33%;
  height:100rpx;
  display: block; 
  box-sizing: border-box;
  float: left;
}

.table {
  border: 0px solid darkgray;
  width: 100%;
  height: 1000rpx; 
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:50%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width:100%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  margin: 60rpx;
}
.bottom{
  width: auto;
}
